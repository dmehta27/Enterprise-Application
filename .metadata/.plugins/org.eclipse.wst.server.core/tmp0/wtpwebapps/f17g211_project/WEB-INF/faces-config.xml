<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    
    <managed-bean>
		<managed-bean-name>dBAccessInfoBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids.model.DbAccessInfoBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dbmsUserBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids.model.DbmsUserBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>messageBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids.model.MessageBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dbActionBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids.model.DbActionBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actionBeanFile</managed-bean-name>
		<managed-bean-class>edu.uic.ids.model.ActionBeanFile</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>messageBean</property-name>
			<property-class>edu.uic.ids.model.MessageBean</property-class>
			<value>#{messageBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actionBeanGraph</managed-bean-name>
		<managed-bean-class>edu.uic.ids.model.ActionBeanGraph</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actionBeanMath</managed-bean-name>
		<managed-bean-class>edu.uic.ids.model.ActionBeanMath</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
    <navigation-rule>
		<from-view-id>/Login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>SUCCESS</from-outcome>
			<to-view-id>/DisplayTables.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>FAIL</from-outcome>
			<to-view-id>/Login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/DisplayTables.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/Logout.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>BACK</from-outcome>
			<to-view-id>/Login.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>GRAPH</from-outcome>
			<to-view-id>/GraphicalAnalysis.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>MATH</from-outcome>
			<to-view-id>/math.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/UploadScript.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/Logout.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>BACK</from-outcome>
			<to-view-id>/DisplayTables.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/math.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/Logout.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>BACK</from-outcome>
			<to-view-id>/DisplayTables.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/GraphicalAnalysis.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/Logout.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>BACK</from-outcome>
			<to-view-id>/DisplayTables.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
